{
  "name": "template",
  "version": "1.0.0",
  "description": "NodeJS with gprc, proto in TS",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run test:mocha",
    "dev": "REDIS_HOST=localhost GRPC_VERBOSITY=DEBUG GRPC_TRACE=all nodemon -e js,ts,tsx src/index.ts",
    "dev:asProd": "REDIS_HOST=localhost GRPC_VERBOSITY=DEBUG GRPC_TRACE=all NODE_ENV=production nodemon -e js,ts,tsx src/index.ts",
    "proto:clear": "rm -rf ./src/proto",
    "proto:build": "sh ./scripts/build-protos.sh ./*.proto ./src/proto ",
    "proto": "npm run proto:clear && npm run proto:build",
    "test:mocha": "REDIS_HOST=localhost mocha --exit -r ts-node/register  ./src/test/*.test.ts",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma": "npm run prisma:pull && npm run prisma:generate",
    "lint": "eslint ."
  },
  "author": "Sebastian Joerz",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.212.0",
    "@aws-sdk/s3-request-presigner": "^3.212.0",
    "@grpc/grpc-js": "^1.7.3",
    "aws-sdk": "^2.1256.0",
    "bcrypt": "^5.1.0",
    "chance": "^1.1.9",
    "google-protobuf": "^3.21.2",
    "grpc-server-reflection": "^0.1.5",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "redis": "^4.4.0",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/chance": "^1.1.3",
    "@types/google-protobuf": "^3.15.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.5",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "chai": "^4.3.7",
    "eslint": "^8.28.0",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.3",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
